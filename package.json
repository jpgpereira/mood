{
  "name": "mood",
  "version": "0.0.1",
  "description": "Hood's mood",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "dependencies": {
    "@slack/client": "^3.10.0",
    "babel-runtime": "^6.20.0",
    "giphy-api": "^1.2.6",
    "material-ui": "^1.0.0-alpha.15",
    "meteor-node-stubs": "~0.2.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-tap-event-plugin": "^2.0.1",
    "yeelight-wifi": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-meteor": "^0.3.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.10.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "settings": {
      "import/resolver": "meteor"
    },
    "env": {
      "browser": true
    },
    "rules": {
      "import/extensions": [
        "off",
        "never"
      ],
      "import/no-extraneous-dependencies": 0,
      "no-underscore-dangle": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpgpereira/mood.git"
  },
  "keywords": [
    "hood",
    "mood",
    "comon",
    "builduplabs",
    "iot"
  ],
  "author": "Jo√£o Pereira <joao@builduplabs.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jpgpereira/mood/issues"
  },
  "homepage": "https://github.com/jpgpereira/mood#readme"
}
